{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "object-curly-newline": "off",
    "consistent-return": "off",
    "implicit-arrow-linebreak": "off"
  }
}
